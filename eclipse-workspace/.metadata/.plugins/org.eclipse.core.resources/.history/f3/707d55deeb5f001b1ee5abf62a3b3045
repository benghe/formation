package fr.formation.model;

public class Produit {

	private String nom;

	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		this.nom = nom;
	}
	
	

	/**
	 * @param nom
	 */
	public Produit(String nom) {
		super();
		this.nom = nom;
	}

	@Override
	public String toString() {
		return "Produit [nom=" + nom + "]";
	}
	
	
}
